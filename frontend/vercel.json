{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js", 
        "use": "@vercel/static-build"
      },
      {
        "src": "api/**/*",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "outputDirectory": "build"
  }
  